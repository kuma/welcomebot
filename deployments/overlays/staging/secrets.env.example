# Welcome Bot - Staging Environment Secrets
# Copy this file to secrets.env and fill in your values
# DO NOT commit secrets.env to Git!

# Master Bot Token (staging)
DISCORD_BOT_TOKEN=your_staging_master_bot_token_here

# Slave Bot Tokens (3 separate bot accounts for staging)
SLAVE_1_TOKEN=your_staging_slave1_bot_token_here
SLAVE_2_TOKEN=your_staging_slave2_bot_token_here
SLAVE_3_TOKEN=your_staging_slave3_bot_token_here

# PostgreSQL Configuration
POSTGRES_USER=welcomebot_staging
POSTGRES_PASSWORD=strong_staging_password
POSTGRES_DB=welcomebot_staging

# Redis Password (optional but recommended for staging)
REDIS_PASSWORD=staging_redis_password

