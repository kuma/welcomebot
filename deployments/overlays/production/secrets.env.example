# Welcome Bot - Production Environment Secrets
# Copy this file to secrets.env and fill in your values
# DO NOT commit secrets.env to Git!

# Master Bot Token (production)
DISCORD_BOT_TOKEN=your_production_master_bot_token_here

# Slave Bot Tokens (3 separate bot accounts for production)
SLAVE_1_TOKEN=your_production_slave1_bot_token_here
SLAVE_2_TOKEN=your_production_slave2_bot_token_here
SLAVE_3_TOKEN=your_production_slave3_bot_token_here

# PostgreSQL Configuration
POSTGRES_USER=welcomebot_prod
POSTGRES_PASSWORD=very_strong_production_password
POSTGRES_DB=welcomebot_prod

# Redis Password (REQUIRED for production)
REDIS_PASSWORD=strong_production_redis_password

